<resources>
    <string name="app_name">BasicsCodelab</string>
    <string name="show_more">Show more</string>
    <string name="show_less">Show less</string>

    <string name="poem_title">Hujan Bulan Juni</string>
    <string name="poem_content"><![CDATA[
        Tak ada yang lebih arif
        Dari hujan bulan Juni
        Dibiarkannya yang tak terucapkan
        Diserap akar pohon bunga itu.
    ]]></string>

    <string name="poem_title_en">Rain in June</string>
    <string name="poem_content_en"><![CDATA[
        Nothing is wiser
        Than the rain in June
        It lets the unspoken words
        Be absorbed by the roots of the flowering tree.
    ]]></string>
</resources>